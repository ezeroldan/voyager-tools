{
    "name": "ezeroldan/voyager-tools",
    "description": "Voyager Tools para mis proyectos",
    "keywords": [
        "ezeroldan",
        "voyager"
    ],
    "homepage": "https://github.com/ezeroldan/voyager-tools",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ezequiel Roldan",
            "email": "ezeroldan@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "artesaos/seotools": "^0.17.2",
        "illuminate/support": "~6.0|~7.0",
        "tcg/voyager": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0",
        "orchestra/testbench": ">=4.0"
    },
    "autoload": {
        "psr-4": {
            "EzeRoldan\\VoyagerTools\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EzeRoldan\\VoyagerTools\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "EzeRoldan\\VoyagerTools\\VoyagerToolsServiceProvider"
            ]
        }
    }
}
